{
  "name": "intelligent-document-processor",
  "version": "1.0.0",
  "description": "Advanced AI-powered document processing application",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build:frontend && npm run copy-frontend && npm run build:backend",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "copy-frontend": "node scripts/copy-frontend.js",
    "start": "cd backend && NODE_ENV=production npm start",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "cd backend && npm test",
    "test:frontend": "cd frontend && npm test",
    "lint": "npm run lint:backend && npm run lint:frontend",
    "lint:backend": "cd backend && npm run lint",
    "lint:frontend": "cd frontend && npm run lint",
    "setup": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "clean": "rimraf backend/dist frontend/dist backend/frontend-build backend/node_modules frontend/node_modules node_modules",
    "postinstall": "cd backend && npm install && cd ../frontend && npm install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/intelligent-document-processor.git"
  },
  "keywords": [
    "document-processing",
    "ai",
    "ocr",
    "template-engine",
    "automation",
    "react",
    "nodejs",
    "typescript"
  ],
  "author": "Document Processor Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.0",
    "rimraf": "^5.0.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}